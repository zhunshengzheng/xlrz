function wapFn(){var e=navigator.userAgent.toLowerCase(),t="ipad"==e.match(/ipad/i),i="iphone os"==e.match(/iphone os/i),a="midp"==e.match(/midp/i),n="rv:1.2.3.4"==e.match(/rv:1.2.3.4/i),r="ucweb"==e.match(/ucweb/i),s="android"==e.match(/android/i),c="windows ce"==e.match(/windows ce/i),o="windows mobile"==e.match(/windows mobile/i);return!!(t||i||a||n||r||s||c||o)}function lsIE9(){return!gtIE9()}function gtIE9(){var e=!0;if("Microsoft Internet Explorer"==navigator.appName){var t=navigator.appVersion.split(";")[1].replace(/[ ]/g,"");-1!=t.indexOf("MSIE")&&(t=parseInt(t.substring(4)))<=9&&(e=!1)}return e}function isWeiXin(){return"micromessenger"==window.navigator.userAgent.toLowerCase().match(/MicroMessenger/i)}function addFile(e,t){var i="";switch(e){case"css":(i=document.createElement("link")).rel="stylesheet",i.type="text/css",i.href=t;break;case"js":(i=document.createElement("script")).type="text/javascript",i.src=t;break;default:alert("没有添加合适的文件类型")}i.media="screen";document.getElementsByTagName("head")[0].appendChild(i)}